  <!-- Simple mailto composer and quick vendor buttons -->
  <script>
    function openMail(e){
      e.preventDefault();
      const f = e.target.closest('form') || document.getElementById('tourForm');
      const name = encodeURIComponent(f.name.value.trim());
      const contact = encodeURIComponent(f.contact.value.trim());
      const date = encodeURIComponent(f.date.value);
      const guests = encodeURIComponent(f.guests.value);
      const msg = encodeURIComponent(f.msg.value.trim());
      const subject = encodeURIComponent('–ó–∞–ø—Ä–æ—Å —Ç—É—Ä–∞: AOP Pic Saint‚ÄëLoup (1 –¥–µ–Ω—å)');
      const body = encodeURIComponent(
        `–ò–º—è: ${name}\n–ö–æ–Ω—Ç–∞–∫—Ç: ${contact}\n–î–∞—Ç–∞: ${date || '‚Äî'}\n–ì–æ—Å—Ç–µ–π: ${guests}\n\n–°–æ–æ–±—â–µ–Ω–∏–µ:\n${msg || '‚Äî'}\n\n–ú–∞—Ä—à—Ä—É—Ç: Hortus ‚Üí –ú–æ–Ω—Ñ–µ—Ä—Ä–∞–Ω ‚Üí Auberge du C√®dre ‚Üí Mas Brugui√®re ‚Üí Lancyre.`
      );
      // TODO: –ø–æ–º–µ–Ω—è–π—Ç–µ email –Ω–∞ –≤–∞—à —Ä–∞–±–æ—á–∏–π –∞–¥—Ä–µ—Å
      window.location.href = `mailto:booking@example.com?subject=${subject}&body=${body}`;
      return false;
    }

    // Quick buttons under timeline
    document.addEventListener('click', (ev)=>{
      const btn = ev.target.closest('button[data-book]');
      if(!btn) return;
      const tag = btn.getAttribute('data-book');
      const names = {
        hortus: 'Domaine de l\u2019Hortus',
        bruguiere: 'Mas Brugui\u00e8re',
        lancyre: 'Ch\u00e2teau de Lancyre',
        cedre: 'Auberge du C\u00e8dre'
      };
      const subject = encodeURIComponent(`–ë—Ä–æ–Ω—å/–≤–∏–∑–∏—Ç: ${names[tag]} ‚Äî Pic Saint‚ÄëLoup`);
      const body = encodeURIComponent('–ó–¥—Ä–∞–≤—Å—Ç–≤—É–π—Ç–µ! –•–æ—á—É –∑–∞–ø–∏—Å–∞—Ç—å—Å—è –Ω–∞ –≤–∏–∑–∏—Ç/—Å—Ç–æ–ª –≤ —É–∫–∞–∑–∞–Ω–Ω—ã–µ –¥–∞—Ç—ã. –ü–æ–¥—Å–∫–∞–∂–∏—Ç–µ –¥–æ—Å—Ç—É–ø–Ω—ã–µ —Å–ª–æ—Ç—ã –∏ —É—Å–ª–æ–≤–∏—è.');
      window.location.href = `mailto:booking@example.com?subject=${subject}&body=${body}`;
    });
  </script>

